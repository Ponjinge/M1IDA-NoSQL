CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

MATCH (s1:Station {Nom: 'PATINOIRE BARRADELS'}), (s2:Station {Nom: 'ODYSSUD RITOURET'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);


