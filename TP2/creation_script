CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);
 
MATCH (s1:Station {Nom: 'ANCELY'}), (s2:Station {Nom: 'ARENES ROMAINES'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);


MATCH (s1:Station {Nom: 'ANCELY'}), (s2:Station {Nom: 'NADOT'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'NADOT'}), (s2:Station {Nom: 'DAURAT'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'DAURAT'}), (s2:Station {Nom: 'AEROPORT'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);


MATCH (s1:Station {Nom: 'ARENES ROMAINES'}), (s2:Station {Nom: 'PURPAN'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);
 
MATCH (s1:Station {Nom: 'PURPAN'}), (s2:Station {Nom: 'CARTOUCHERIE'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'CARTOUCHERIE'}), (s2:Station {Nom: 'ZENITH'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'ZENITH'}), (s2:Station {Nom: 'ARENES'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
