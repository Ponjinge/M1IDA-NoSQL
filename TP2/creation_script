WHERE s.Nom='AEROCONSTELLATION'

SET s:Station:Terminus;





 
--7)

 
MATCH (s1:Station {Nom: 'AEROCONSTELLATION'}), (s2:Station {Nom: 'BEAUZELLE - AEROSCOPIA'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'BEAUZELLE - AEROSCOPIA'}), (s2:Station {Nom: 'ANDROMEDE LYCEE'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
MATCH (s1:Station {Nom: 'ANDROMEDE LYCEE'}), (s2:Station {Nom: 'PLACE GEORGES BRASSENS'})

CREATE (s1)-[:Suivant]->(s2), (s2)-[:Précédent]->(s1);

 
